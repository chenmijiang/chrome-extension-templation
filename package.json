{
  "name": "chrome-extension-templation",
  "version": "0.1.0",
  "description": "Chrome Extension Templation with React, TypeScript, TailwindCSS, Styled-Components, Antd, and Webpack",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "pack": "node pack.js",
    "repack": "npm run build && npm run pack",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\""
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@types/chrome": "^0.0.181",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "adm-zip": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.10.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.10",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "styled-components": "^6.1.8",
    "ts-loader": "^9.2.8",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^4.10.0",
    "webpack-ext-reloader": "^1.1.12",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "antd": "^5.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "valtio": "^1.13.1"
  }
}
